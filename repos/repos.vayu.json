[
  {
    "kernelSource": {
      "name": "Perry-RED",
      "repo": "https://github.com/Sizo69/android_kernel_xiaomi_sm8150.git",
      "branch": "main",
      "device": "vayu",
      "defconfig": "vayu_user_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-r498229b.git",
        "branch": "14.0",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android10-release",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LLVM": "1",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Sizo69/AnyKernel3.git",
      "branch": "master"
    }
  }
]
