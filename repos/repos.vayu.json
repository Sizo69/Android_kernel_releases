[
  {
    "kernelSource": {
      "name": "AstraL-Kernel",
      "repo": "https://github.com/Sizo69/kernel_AstraL_mondrian.git",
      "branch": "main",
      "device": "mondrian",
      "defconfig": "vendor/mondrian_defconfig",
      "DLKM": {
        "use": true,
        "commands": [
          "make O=out ARCH=arm64 vendor_dlkm.img"
        ]
      },
      "Ramdisk": {
        "use": true,
        "commands": [
          "mkbootimg --kernel out/arch/arm64/boot/Image",
          "--ramdisk out/ramdisk.img",
          "--output out/boot.img"
        ]
      }
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://gitlab.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-r498229b.git",
        "branch": "14.0",
        "name": "clang",
        "binPath": ["bin"]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android10-release",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LLVM": "1",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Sizo69/AnyKernel3_mondrian.git",
      "branch": "master"
    }
  }
]
